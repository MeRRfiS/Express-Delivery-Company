<Window x:Class="Express_Delivery.Surrender"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Express_Delivery"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Surrender" Height="130.342" Width="373.68" WindowStartupLocation="CenterScreen">
    <Grid Background="LightGray">
        <StackPanel Margin="0,0,-0.4,-0.4" Grid.ColumnSpan="2">
            <TextBox Margin="10 10 10 10" x:Name="textBoxMoney" HorizontalAlignment="Left" Width="357" Height="27" Background="White" BorderBrush="#FF8AC4D0" materialDesign:HintAssist.Hint="Отримана сума" FontSize="22.5" Padding="8,0,0,0" FontFamily="Roboto" Style="{StaticResource MaterialDesignTextBox}"/>
            <Button x:Name="button_count"  Height="38" Content="Вирахувати решту" FontFamily="Global User Interface" Background="#F4D160" FontSize="22.5" Foreground="Black" BorderThickness="5" BorderBrush="#FFFBEEAC" Margin="10,0,10,10" Click="Button_Count_Click"/>
        </StackPanel>

    </Grid>
</Window>
